<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AgendaCalendarView">
        <attr name="agendaCurrentDayTextColor" format="color" />
        <attr name="agendaCurrentDayColor" format="color" />
        <attr name="fabColor" format="color" />
        <attr name="calendarNoEventText" format="string" />
        <attr name="showNoEventText" format="boolean" />
        <attr name="agendaHeaderStyle" format="enum">
            <enum name="compact" value="0"/>
            <enum name="wide" value="1"/>
        </attr>
        <attr name="agendaHeaderDecoration" format="reference"/>


        <attr name="calendarHeaderColor" format="color" />
        <attr name="calendarColor" format="color" />
        <attr name="calendarDayTextColor" format="color" />
        <attr name="calendarPastDayTextColor" format="color" />
        <attr name="calendarCurrentDayTextColor" format="color" />
        <attr name="calendarCurrentDayCircleColor" format="color" />
    </declare-styleable>
</resources>
